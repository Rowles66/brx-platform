{
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "next dev",
    "build": "next build",
    "build:local": "next build",
    "start": "next start",
    "lint": "next lint",
    "preview": "concurrently \"npm run dev\" \"npm run preview:sync\"",
    "preview:sync": "browser-sync start --config preview.config.js",
    "preview:open": "concurrently \"npm run dev\" \"npm run preview:sync\" \"sleep 5 && open http://localhost:3000 http://localhost:3001\"",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "schema:generate": "node scripts/generate-unified-schema.js",
    "schema:analyze": "node scripts/generate-unified-schema.js && cat prisma/unified-schema.prisma",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dev:debug": "NODE_OPTIONS='--inspect-brk' next dev",
    "vercel:dev": "vercel dev --debug --listen 3000",
    "vercel:build": "vercel build --debug",
    "monitor:deployment": "bash scripts/monitor-deployment.sh",
    "audit:ai": "node scripts/ai-audit/run-audit.js",
    "audit:visual": "playwright test --grep visual",
    "audit:playwright": "node scripts/ai-audit/run-playwright-audit.js",
    "audit:vercel": "node vercel-visual-audit.js",
    "deploy:audit": "node deploy-with-audit.js",
    "deploy:prod": "vercel --prod",
    "deploy:preview": "vercel",
    "quick:screenshot": "node quick-screenshot.js",
    "audit:api": "curl -s https://brx-app-replica-brx-performance.vercel.app/api/visual-audit?action=audit | jq",
    "test:deployment": "node test-deployment.js"
  },
  "dependencies": {
    "@ai-sdk/vercel": "0.0.1",
    "@applitools/eyes-playwright": "1.37.2",
    "@prisma/client": "^5.0.0",
    "@tanstack/react-query": "4.29.19",
    "@tanstack/react-query-devtools": "4.39.0",
    "@trpc/client": "10.34.0",
    "@trpc/next": "10.34.0",
    "@trpc/react-query": "10.34.0",
    "@trpc/server": "10.34.0",
    "ai": "4.3.16",
    "jimp": "1.6.0",
    "lucide-react": "^0.279.0",
    "next": "15.3.3",
    "openai": "5.1.0",
    "pg": "^8.11.0",
    "pixelmatch": "7.1.0",
    "pngjs": "7.0.0",
    "prisma": "^5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.34.2",
    "superjson": "^1.13.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "1.52.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/nextjs": "^7.0.0",
    "@storybook/react": "7.6.20",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/postcss": "4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "18.19.110",
    "@types/pg": "^8.10.2",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "10.4.21",
    "browser-sync": "^2.29.3",
    "chromatic": "12.1.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-storybook": "^0.6.12",
    "jsdom": "^26.1.0",
    "postcss": "8.5.4",
    "source-map-support": "^0.5.21",
    "tailwindcss": "3.4.14",
    "typescript": "5.8.3",
    "vitest": "^3.1.4"
  }
}
