{
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "next dev",
    "build": "next build",
    "build:local": "next build",
    "start": "next start",
    "lint": "next lint",
    "preview": "concurrently \"npm run dev\" \"npm run preview:sync\"",
    "preview:sync": "browser-sync start --config preview.config.js",
    "preview:open": "concurrently \"npm run dev\" \"npm run preview:sync\" \"sleep 5 && open http://localhost:3000 http://localhost:3001\"",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "schema:generate": "node scripts/generate-unified-schema.js",
    "schema:analyze": "node scripts/generate-unified-schema.js && cat prisma/unified-schema.prisma",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dev:debug": "NODE_OPTIONS='--inspect-brk' next dev",
    "vercel:dev": "vercel dev --debug --listen 3000",
    "vercel:build": "vercel build --debug",
    "monitor:deployment": "bash scripts/monitor-deployment.sh"
  },
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/material": "5.13.0",
    "@prisma/client": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@tanstack/react-query": "4.29.19",
    "@tanstack/react-query-devtools": "4.39.0",
    "@trpc/client": "10.34.0",
    "@trpc/next": "10.34.0",
    "@trpc/react-query": "10.34.0",
    "@trpc/server": "10.34.0",
    "lucide-react": "^0.279.0",
    "next": "13.4.19",
    "pg": "^8.11.0",
    "prisma": "^5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.13.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/nextjs": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^18.16.0",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "10.4.14",
    "browser-sync": "^2.29.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-storybook": "^0.6.12",
    "jsdom": "^26.1.0",
    "postcss": "8.4.14",
    "source-map-support": "^0.5.21",
    "tailwindcss": "3.3.0",
    "typescript": "^5.0.4",
    "vitest": "^3.1.4"
  }
}
